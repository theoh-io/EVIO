{
  
  "dataset": {
    "type": "uzh_fpv",
    "batch_size": 16,
    "img_nc": 3,
    "image_size": [346, 260],
    "imu_dim": 8,
    "event_dim":4,
    "target_nc": 8,
    "serial_batches": false,
    "drop_last_batch": true,
    "n_threads": 1
  },
  "dataset_train": {
    "n_threads": 3
  },
  "dataset_val": {
    "serial_batches": true
  },
  "dataset_test": {
    "batch_size": 1,
    "drop_last_batch": false,
    "n_prev_frames": 0,
    "serial_batches": true
  },

  "model": {
    "load_epoch": -1,
    "type": "model3",
    "master_gpu": 0,
    "reg_gpus": [0]
  },

  "train": {
    "nepochs_no_decay": 7,
    "nepochs_decay": 3,
    "reg_lr": 0.1,
    "num_iters_validate": 10
  },

  "logs": {
    "print_freq_s": 5,
    "display_freq_s": 180,
    "save_latest_freq_s": 3600
  },

  "networks": {
    "reg": {
      "type": "multimodal",
      "hyper_params": {
        "inputs": ["images", "events", "imu"],
        "input_dims":[[3, 346, 260],4,8],
        "output_dim": 8
      },
      "encoders": {
        "img_encoder": "resnet18",
        "ev_encoder": "linear",
        "imu_encoder": "wavenet"
      },
      "fusion": {
        "fusion_method": "linear"
      }
    }
  },

  "transforms_train": ["to_tensor"],
  "transforms_val": ["to_tensor"],
  "transforms_test": ["to_tensor"],
  "transforms": {
    "to_tensor": {
      "type": "to_tensor",
      "general_args": {},
      "perkey_args": {
        "img": {}
      }
    },
    "normalize": {
      "type": "normalize",
      "general_args": {
        "mean": [0.485, 0.456, 0.406],
        "std": [0.229, 0.224, 0.225]
      },
      "perkey_args": {
        "img": {}
      }
    },
    "horz_flip": {
      "type": "rand_horz_flip",
      "general_args": {},
      "perkey_args": {
        "img": {}
      }
    }
  }
}